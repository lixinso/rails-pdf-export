<h1>Listing pdf3s</h1>

<table>
  <tr>
    <th>Htmlcontent</th>
  </tr>

<% @pdf3s.each do |pdf3| %>
  <tr>
    <td><%=h pdf3.htmlcontent %></td>
    <td><%= link_to 'Show', pdf3 %></td>
    <td><%= link_to 'Edit', edit_pdf3_path(pdf3) %></td>
    <td><%= link_to 'Destroy', pdf3, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New pdf3', new_pdf3_path %>